<?xml version="1.0"?>

<launch>

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find urdf_description)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find urdf_description)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find urdf_description)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find urdf_description)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find urdf_description)/config/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find urdf_description)/config/global_planner_params.yaml" command="load" />

 </node>

</launch> 